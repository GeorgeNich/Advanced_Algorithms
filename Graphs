   "source": [
    "Now let us see if we can do BFS on the graph"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "id": "engaging-fountain",
   "metadata": {},
   "outputs": [],
   "source": [
    "# def BFS(root):\n",
    "\n",
    "#     visited = [False] * (num_nodes_in_graph)\n",
    "\n",
    "#     # Create a queue for BFS\n",
    "#     queue = []\n",
    "\n",
    "#     queue.append(root)\n",
    "#     visited[root] = True  #changed the visited root to true\n",
    "\n",
    "#     while queue:\n",
    "\n",
    "#         r = queue.pop(0)\n",
    "#         print(r)\n",
    "\n",
    "#         for n in r.adjacent:\n",
    "            \n",
    "#             if visited[n] == False:\n",
    "                \n",
    "#                 visited[n] = True\n",
    "#                 queue.append(n)\n",
    "                "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "id": "illegal-tourist",
   "metadata": {},
   "outputs": [],
   "source": [
    "def BFS(w, G):\n",
    "    \n",
    "    for v in G.vertices:\n",
    "        v.status = \"unvisited\"\n",
    "        \n",
    "    n = len(G.vertices)\n",
    "    \n",
    "    Ls = [ [] for i in range(n) ]\n",
    "    \n",
    "    Ls[0] = [w]\n",
    "    w.status = \"visited\"\n",
    "    \n",
    "    for i in range(n):\n",
    "        for u in Ls[i]:\n",
    "            \n",
    "            print(u)\n",
    "            \n",
    "            for v in u.getOutNeighbors():\n",
    "                \n",
    "                if v.status == \"unvisited\":\n",
    "                    v.status = \"visited\"\n",
    "                    Ls[i + 1].append(v)"
   ]
  },
  {
