   "source": [
    "def pre_order(G2, w):\n",
    "    if w is None:\n",
    "        return\n",
    "    print(w)\n",
    "    if len(w.getOutNeighbors()) == 0:\n",
    "        pre_order(G2, None)\n",
    "        pre_order(G2, None)\n",
    "    if len(w.getOutNeighbors()) == 1:\n",
    "        pre_order(G2, w.getOutNeighbors()[0])\n",
    "        pre_order(G2, None)\n",
    "    if len(w.getOutNeighbors()) == 2:\n",
    "        pre_order(G2, w.getOutNeighbors()[0])\n",
    "        pre_order(G2, w.getOutNeighbors()[1])\n",
    "\n",
    "        \n",
    "def post_order(G2, w):\n",
    "    if w is None:\n",
    "        return\n",
    "    if len(w.getOutNeighbors()) == 0:\n",
    "        post_order(G2, None)\n",
    "        in_order(G2, None)\n",
    "    if len(w.getOutNeighbors()) == 1:\n",
    "        post_order(G2, w.getOutNeighbors()[0])\n",
    "        post_order(G2, None)\n",
    "    if len(w.getOutNeighbors()) == 2:\n",
    "        post_order(G2, w.getOutNeighbors()[0])\n",
    "        post_order(G2, w.getOutNeighbors()[1])\n",
    "    print(w)\n",
    "    \n",
    "\n",
    "def in_order(G2, w):\n",
    "    if w is None:\n",
    "        return\n",
    "    if len(w.getOutNeighbors()) == 0:\n",
    "        in_order(G2, None)\n",
    "        print(w)\n",
    "        in_order(G2, None)\n",
    "    if len(w.getOutNeighbors()) == 1:\n",
    "        in_order(G2, w.getOutNeighbors()[0])\n",
    "        print(w)\n",
    "        in_order(G2, None)\n",
    "    if len(w.getOutNeighbors()) == 2:\n",
    "        in_order(G2, w.getOutNeighbors()[0])\n",
    "        print(w)\n",
    "        in_order(G2, w.getOutNeighbors()[1])\n",
    "\n"
   ]
  },
