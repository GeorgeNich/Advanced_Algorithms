   "cell_type": "code",
   "execution_count": 29,
   "id": "cc043692",
   "metadata": {},
   "outputs": [],
   "source": [
    "def BFS_depth_bipartite(root, Graph):\n",
    "    for node in Graph.vertices:\n",
    "        node.status = \"unvisited\"\n",
    "        node.color = '-'\n",
    "\n",
    "    root.color = 'r'\n",
    "    visited = []\n",
    "\n",
    "    visited.append(root)\n",
    "    root.status = \"visited\"\n",
    "    while len(visited) > 0:             #first element is vertex so vertex is parent\n",
    "        vertex = visited.pop()\n",
    "        color_of_parent = vertex.color\n",
    "        for node in vertex.getOutNeighbors():\n",
    "            if node.status == \"unvisited\":\n",
    "                if node.color == '-':\n",
    "                    node.color = 'b' if color_of_parent == 'r' else 'r'\n",
    "                elif node.color == color_of_parent:\n",
    "                    print('Not a bipartide Graph')\n",
    "                    return False\n",
    "                node.status = \"visited\"\n",
    "                visited.append(node)\n",
    "            elif node.color == color_of_parent:\n",
    "                print('Not a bipartide Graph')\n",
    "                return False\n",
    "    # --\n",
    "    print('Yes this is a bipartide Graph')\n",
    "    return True"
   ]
  },
