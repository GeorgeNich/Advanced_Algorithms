     ]
    }
   ],
   "source": [
    "# Let us do the DFS from Melbourne\n",
    "w = G.vertices[0]\n",
    "\n",
    "DFS(w, G)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "id": "deea91b3",
   "metadata": {},
   "outputs": [],
   "source": [
    "def display_graph(G):\n",
    "    V = G.vertices\n",
    "    dot = Digraph()\n",
    "    print(G)\n",
    "    for a,b in G.getDirEdges():\n",
    "        dot.edge(str(a),str(b))\n",
    "    # --\n",
    "    return dot"
   ]
  },
